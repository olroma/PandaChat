<Window x:Class="PandaChatClient.ChangeLogWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PandaChatClient"
        mc:Ignorable="d"
        Title="ChangeLogWin" Height="460.5" Width="390" Background="#FF1D1D1D" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBox x:Name="ChangeLogBox" HorizontalAlignment="Left" Height="405" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="362" Background="#FF1D1D1D" Foreground="White" FontFamily="Consolas" FontWeight="Bold"/>
        <Button x:Name="OKButton" Content="Продолжить" Margin="10,420,18,10" Background="#FF25935A" Foreground="White" FontFamily="Consolas" FontWeight="Bold"/>

    </Grid>
</Window>
